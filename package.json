{
  "name": "genuss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build && xcopy \".next\\static\" \".next\\standalone\\.next\\static\" /E /I /H /Y && xcopy \"public\" \".next\\standalone\\public\" /E /I /H /Y",
    "build:standalone": "cross-env NODE_ENV=production next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "deploy": "npm run build:standalone",
    "start": "node .next/standalone/server.js",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "lint": "eslint",
    "docker:dev": "docker-compose up --build",
    "docker:dev:down": "docker-compose down",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down",
    "docker:logs:dev": "docker-compose logs -f",
    "docker:logs:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml logs -f",
    "docker:build:dev": "docker-compose build",
    "docker:build:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "docker:clean": "docker system prune -f"
  },
